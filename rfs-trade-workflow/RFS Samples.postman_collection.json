			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"try{\r",
							"var jsonData1 = pm.response.json();\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test('Test RFS Fetch Quote Request placed successfully', function() {\r",
							"    const jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.clOrderId).to.not.be.eql(null); \r",
							"    pm.expect(jsonData.requestId).to.not.be.eql(null); \r",
							"    pm.expect(jsonData.transactionId).to.not.be.eql(null);   \r",
							"    pm.expect(jsonData.ttl).to.be.gte(120);          \r",
							"    if(jsonData.event == \"QUOTE\")\r",
							"    {\r",
							"        pm.collectionVariables.set(\"quoteId\",jsonData.quotes[0].bids[0].quoteId);\r",
							"        pm.expect(jsonData.quotes[0].requestId).to.be.not.eql(null);\r",
							"        pm.expect(jsonData.quotes[0].priceType).to.be.eql(\"FwdFwd\");\r",
							"        pm.expect(jsonData.quotes[0].effectiveTime).to.be.not.eql(null);\r",
							"        pm.expect(jsonData.quotes[0].symbol).to.be.eql(\"EUR/USD\");\r",
							"        pm.expect(jsonData.quotes[0].ttl).to.be.not.eql(null);\r",
							"        pm.expect(jsonData.quotes[0].status).to.be.oneOf([\"A\",\"B\"]);\r",
							"        pm.expect(jsonData.quotes[0].nearValueDate).to.be.not.eql(null);\r",
							"        pm.expect(jsonData.quotes[0].farValueDate).to.be.not.eql(null);\r",
							"        for(i=0; i<jsonData.quotes[0].bids.length; i++)\r",
							"        {\r",
							"        pm.expect(jsonData.quotes[0].bids[i].legType).to.be.oneOf([0,1]);\r",
							"        pm.expect(jsonData.quotes[0].bids[i].type).to.be.eql(\"BID\");\r",
							"        pm.expect(jsonData.quotes[0].bids[i].quoteId).to.be.not.eql(null);\r",
							"        pm.expect(jsonData.quotes[0].bids[i].dealtAmount).to.be.gte(1000000.0);\r",
							"        pm.expect(jsonData.quotes[0].bids[i].settledAmount).to.be.gte(1000000.0);\r",
							"        pm.expect(jsonData.quotes[0].bids[i].provider).to.be.not.eql(null);\r",
							"        pm.expect(jsonData.quotes[0].bids[i].rate).to.be.not.eql(null);\r",
							"        pm.expect(jsonData.quotes[0].bids[i].spotRate).to.be.not.eql(null);\r",
							"        pm.expect(jsonData.quotes[0].bids[i].forwardPoint).to.be.not.eql(null);\r",
							"        pm.expect(jsonData.quotes[0].bids[i].midRate).to.be.not.eql(null);\r",
							"        if (jsonData.quotes[0].bids[i].legType==1)\r",
							"        {\r",
							"        pm.expect(jsonData.quotes[0].bids[i].dealtAmount).to.be.gte(2000000.0);            \r",
							"        }\r",
							"\r",
							"        }\r",
							"        //Offer side validations\r",
							"        for(i=0; i<jsonData.quotes[0].offers.length; i++)\r",
							"        {\r",
							"        pm.expect(jsonData.quotes[0].offers[i].legType).to.be.oneOf([0,1]);\r",
							"        pm.expect(jsonData.quotes[0].offers[i].type).to.be.eql(\"OFFER\");\r",
							"        pm.expect(jsonData.quotes[0].offers[i].quoteId).to.be.not.eql(null);\r",
							"        pm.expect(jsonData.quotes[0].offers[i].dealtAmount).to.be.gte(1000000.0);\r",
							"        pm.expect(jsonData.quotes[0].offers[i].settledAmount).to.be.gte(1000000.0);\r",
							"        pm.expect(jsonData.quotes[0].offers[i].provider).to.be.not.eql(null);\r",
							"        pm.expect(jsonData.quotes[0].offers[i].rate).to.be.not.eql(null);\r",
							"        pm.expect(jsonData.quotes[0].offers[i].spotRate).to.be.not.eql(null);\r",
							"        pm.expect(jsonData.quotes[0].offers[i].forwardPoint).to.be.not.eql(null);\r",
							"        pm.expect(jsonData.quotes[0].offers[i].midRate).to.be.not.eql(null);\r",
							"        if (jsonData.quotes[0].offers[i].legType==1)\r",
							"        {\r",
							"        pm.expect(jsonData.quotes[0].offers[i].dealtAmount).to.be.gte(2000000.0);            \r",
							"        }\r",
							"\r",
							"        }\r",
							"        \r",
							"    }\r",
							"    else\r",
							"    {     pm.expect(jsonData.event).to.be.eql('REQUEST_ACCEPTED');      \r",
							"    }\r",
							"});\r",
							"\r",
							"setTimeout(function(){}, 1500);\r",
							"}\r",
							"catch(Error){\r",
							"    console.log(\"response body is null\");\r",
							"}"
